        "title": "📝 文档",
        "enable": true
      },
      "style": {
        "title": "💄 风格",
        "enable": true
      },
      "test": {
        "title": "✅ 测试",
        "enable": true
      },
      "build": {
        "title": "👷‍ 构建",
        "enable": true
      },
      "ci": {
        "title": "🔧 CI 配置",
        "enable": true
      },
      "chore": {
        "title": "🎫 其他更新",
        "enable": true
      }
    }
