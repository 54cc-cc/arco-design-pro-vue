{
  "name": "arco-design-pro-vue",
  "version": "0.0.1-beta",
  "author": {
    "name": "LLiuHuan",
    "email": "184555556@qq.com",
    "url": "https://github.com/LLiuHuan/arco-design-pro-vue"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "changelog": "conventional-changelog -p custom-config -i CHANGELOG.md -s -r 0 -n ./changelog-option.js"
  },
  "private": true,
  "dependencies": {
    "@types/node": "^16.11.6",
    "conventional-changelog-cmyr-config": "^1.2.3",
    "date-fns": "^2.25.0",
    "vue": "^3.2.16",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@arco-design/web-vue": "^2.1.0",
    "@tailwindcss/postcss7-compat": "^2.2.17",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "@vitejs/plugin-vue": "^1.9.3",
    "@vue/eslint-config-prettier": "^6.0.0",
    "autoprefixer": "^9",
    "commitizen": "^4.2.4",
    "compare-func": "^2.0.0",
    "conventional-changelog-cli": "^2.1.1",
    "conventional-changelog-custom-config": "^0.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "less": "^4.1.2",
    "postcss": "^7",
    "prettier": "^2.4.1",
    "scss": "^0.2.4",
    "stylelint": "^14.0.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^2.5.0",
    "stylelint-config-standard": "^23.0.0",
    "stylelint-scss": "^4.0.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "typescript": "^4.4.3",
    "vite": "^2.6.4",
    "vite-plugin-eslint": "^1.3.0",
    "vue-eslint-parser": "^8.0.1",
    "vue-tsc": "^0.3.0"
  },
  "changelog": {
    "authorName": true,
    "authorEmail": true,
    "language": "zh",
    "settings": {
      "feat": {
        "title": "✨ 新功能",
        "enable": true
      },
      "fix": {
        "title": "🐛 Bug 修复",
        "enable": true
      },
      "perf": {
        "title": "⚡ 性能优化",
        "enable": true
      },
      "revert": {
        "title": "⏪ 回退",
        "enable": true
      },
      "refactor": {
        "title": "♻ 代码重构",
        "enable": true
      },
      "docs": {
        "title": "📝 文档",
        "enable": true
      },
      "style": {
        "title": "💄 风格",
        "enable": true
      },
      "test": {
        "title": "✅ 测试",
        "enable": true
      },
      "build": {
        "title": "👷‍ 构建",
        "enable": true
      },
      "ci": {
        "title": "🔧 CI 配置",
        "enable": true
      },
      "chore": {
        "title": "🎫 其他更新",
        "enable": true
      }
    }
  },
  "lint-staged": {
    "*.{vue,js,ts,tsx}": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "keywords": [
    "vue",
    "typescript",
    "vue3",
    "ts",
    "admin"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LLiuHuan/arco-design-pro-vue/issues",
    "email": "184555556@qq.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LLiuHuan/arco-design-pro-vue.git"
  },
  "homepage": "https://github.com/LLiuHuan/arco-design-pro-vue",
  "engines": {
    "node": "^12 || >=14"
  }
}
